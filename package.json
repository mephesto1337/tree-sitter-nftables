{
  "name": "tree-sitter-nftables",
  "version": "1.0.0",
  "description": "tree-sitter parser for nftables rules",
  "main": "index.js",
  "types": "bindings/node",
  "scripts": {
    "test": "tree-sitter test",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "keywords": [
    "tree-sitter",
    "nftables",
    "parser",
    "rules"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "author": "Thomas WACHE",
  "license": "ISC",
  "dependencies": {
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "prebuildify": "^6.0.0"
  },
  "tree-sitter": [
    {
      "scope": "source.nftables",
      "injection-regex": "^nftables$",
      "file-types": [
        "nft",
        "nftables.conf"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "first-line-regex": "^#!.*\\bnft\\s+-f$"
    }
  ]
}
